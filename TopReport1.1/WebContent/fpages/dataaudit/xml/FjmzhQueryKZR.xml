<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="非居民账户个人信息" navigate="非居民账户个人信息" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.getter.FjmzhQueryKZRGetter">
	<Include id="BankParam" />

	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示-->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="select" desc="选择" edittype="checkbox" datatype="boolean" xpath="/select" />
		<!-- 控制人信息 -->
		<Field id="kzriAccID" desc="账号" edittype="text" datatype="string" size="32" require="false" readonly="true" xpath="/kzriAccID" />
		<Field id="kzrCtrlgPersonType" desc="控制人类型" edittype="select" translated="DATA_DIC.300024" datatype="string" size="10" require="false" xpath="/kzrCtrlgPersonType" />
		<Field id="kzrdBirthDate" desc="出生日期" edittype="text" datatype="string" size="10" require="false" xpath="/kzrdBirthDate" />
		<Field id="kzrBirthCity" desc="出生城市 " edittype="text" datatype="string" size="200" require="false" xpath="/kzrBirthCity" />
		<Field id="kzrBirthCountryCode" desc="出生国家(地区)代码" edittype="text" datatype="string" size="3" require="false" xpath="/kzrBirthCountryCode" />
		<Field id="kzrFormerCountryName" desc="出生国家(地区)名称" edittype="text"  datatype="string" size="200" require="false" xpath="/kzrFormerCountryName" />
		<Field id="kzrNationality" desc="国籍" edittype="text"  datatype="string" size="3" require="false" xpath="/kzrNationality" />
		<Field id="kzrResCountryCode" desc="税收居民国（地区）代码" edittype="text" datatype="string" size="3" require="false" xpath="/kzrResCountryCode" />
		<Field id="kzrIssuedBy" desc="发放识别号的国家（地区）代码" edittype="text" datatype="string" size="3" require="false" xpath="/kzrIssuedBy" />
		<Field id="kzrsinType" desc="sinType" edittype="select" translated="LIST:TIN,TIN" datatype="string" size="3" require="false" xpath="/kzrsinType" readonly="false" />
		<Field id="kzrTIN" desc="纳税人识别号" edittype="text" datatype="string" size="200" require="false" xpath="/kzrTIN" readonly="false" />
		<Field id="kzrExplanation" desc="原因" edittype="text" datatype="string" size="1000" require="false" xpath="/kzrExplanation" />
		<Field id="operation" desc="操作" />
	</Fields>

	<Where>
	</Where>

	<Operations>
	
		<Button id="btMod" name="btMod" desc="修改" 
				operation="" url="#" show="" group="" txn=""  icon="icon-edit"/>
		<Button id="btModDZ" name="btMod" desc="查看地址信息" 
				operation="" url="#" show="" group="" txn=""  icon="icon-edit"/>
		<Button id="btModXM" name="btMod" desc="查看姓名信息" 
				operation="" url="#" show="" group="" txn=""  icon="icon-edit"/>		
		<Button id="btNew" name="btNew" desc ="CurrencyManEntry.button.btNew" operation="addrecord" url="#" show="" group="" txn="" icon="icon-add"/>
		<Button id="btSave" name="btSave" desc="CurrencyManEntry.button.btSave"
			operation="asysubmit"
			updateClass="com.huateng.report.imports.updater.FjmzhAddKZR"
			url="/fpages/dataaudit/ftl/FjmzhQueryGR.ftl" txn="" />
			
		<Button id="btSaveEdit" name="btSaveEdit" desc="CurrencyManEntry.button.btSave"
			operation="asysubmit"
			updateClass="com.huateng.report.imports.updater.FjmzhUpdateKZR"
			url="" txn="" />
		<Button id="btBack" name="btBack" desc="返回" operation=""
			url="#" txn="" />		
	</Operations>
	
	

	<Script></Script>

</CommQry>