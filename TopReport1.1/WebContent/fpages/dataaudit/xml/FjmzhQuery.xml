<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="非居民账户查询" navigate="非居民账户查询" type="call"
	interface="false" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	getterclassname="com.huateng.report.getter.FjmzhQueryGetter">
	<Include id="BankParam" />

	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示-->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="select" desc="选择" edittype="checkbox" datatype="boolean" xpath="/select" />
		<Field id="accountNumber" desc="账号" edittype="text" datatype="string" size="20" require="false" xpath="/accountNumber" />
		<Field id="sClosedAccount" desc="账户状态" edittype="select" translated="DATA_DIC.300018" datatype="string" size="20" require="false" xpath="/sClosedAccount" />
		<Field id="sDueDiligenceInd" desc="是否新开客户" edittype="select" translated="DATA_DIC.300019" datatype="string" size="20" require="false" xpath="/sDueDiligenceInd" />
		<Field id="sSelfSertification" desc="是否取得自证声明 " edittype="select" translated="DATA_DIC.300020" datatype="string" size="20" require="false" xpath="/sSelfSertification" />
		<Field id="iAccountBalance" desc="账户余额" edittype="text" datatype="string" size="20" require="false" xpath="/iAccountBalance" />
		<Field id="sACC_currCode" desc="指定货币代码" edittype="select" translated="DATA_DIC.300030" datatype="string" size="20" require="false" xpath="/sACC_currCode" />
		<Field id="sAccountHolderType" desc="账户持有人类别" edittype="select" translated="DATA_DIC.300021" datatype="string" size="20" require="false" xpath="/sAccountHolderType" />
		<Field id="sOpeningFIName" desc="开户银行金融机构编码" edittype="text" datatype="string" size="20" require="false" xpath="/sOpeningFIName" />
		<Field id="sAccountType" desc="账户数据类型" edittype="select" translated="DATA_DIC.300019" datatype="string" size="20" require="false" xpath="/sAccountType" />
		<Field id="sPaymentType" desc="收入类型" edittype="select" translated="LIST:CRS502,利息" datatype="string" size="20" require="false" xpath="/sPaymentType" readonly="false" />
		<Field id="iPaymentAmnt" desc="账户收入金额" edittype="text" datatype="string" size="20" require="false" xpath="/iPaymentAmnt" readonly="false" />
		<Field id="sPaymentAmntCurr" desc="账户收入币种" edittype="select" translated="DATA_DIC.300030" datatype="string" size="20" require="false" xpath="/sPaymentAmntCurr" />
		<Field id="sResCountryCode" desc="税收居民国（地区）代码" edittype="text"  datatype="string" size="20" require="false" xpath="/sResCountryCode" />
		<Field id="sIssuedBy" desc="发放识别号的国家（地区）代码" edittype="text"  datatype="string" size="20" require="false" xpath="/sIssuedBy" />
		<Field id="sinType" desc="sinType " edittype="select" translated="LIST:TIN" datatype="string" size="20" require="false" xpath="/sinType" />
		<Field id="TIN" desc="纳税人识别号" edittype="text" datatype="string" size="20" require="false" xpath="/TIN" />
		<Field id="report_status" desc="报送状态" edittype="select" translated="DATA_DIC.300029" datatype="string" size="20" require="false" xpath="/report_status" />
		<Field id="acType" desc="数据类型" edittype="text" datatype="string" size="20" require="false"  />
	</Fields>
	<Parameters>
	
	</Parameters>
	<Where>
		<Option id="sAccountType"  default="" desc="数据类型" type="string" size="20"  index="3" translated="DATA_DIC.300019"/>
		<Option id="report_status"  default="" desc="报送状态" type="string" size="20" tip="" method="None" index="3" translated="DATA_DIC.300029"/>
		<TextBox id="accountNumber" desc="账号" datatype="string" index="2" size="20"/>
		<Option id="sAccountHolderType"  default="" desc="账户持有人类别" type="string" size="20" tip="" method="None" index="3" translated="DATA_DIC.300021"/>
	</Where>

	<Operations>
		<Button id="btSave" name="btSave" desc="导出报文"
				operation="asysubmit" url="#" updateClass="com.huateng.report.imports.updater.FjmzhQueryOutput" show="" group="" txn=""  icon="icon-remove"/>
		<Button id="btImport" name="btSave" desc="无数据导出"
				operation="asysubmit" url="#" updateClass="com.huateng.report.imports.updater.FjmzhQueryOutput" show="" group="" txn=""  icon="icon-remove"/>
    	<Button id="btLoad" name="btLoad" desc="下载报文" 
				operation="asysubmit" url="#" updateClass="com.huateng.report.imports.updater.FjmzhQueryLoad" show="" group="" txn=""  icon="icon-remove"/>
		<Button id="btAdd" name="btAdd" desc="添加" 
				operation="href" url="/fpages/regonization/ftl/FjmzhAdd.ftl" show="" group="" txn=""  icon="icon-add"/>
		<Button id="btMod" name="btMod" desc="修改" 
				operation="" url="" show="" group="" txn=""  icon="icon-edit"/>
		<Button id="btDel" name="btDel" desc="删除" 
				operation="asysubmit" url="#" updateClass="com.huateng.report.imports.updater.FjmzhQueryDelete" show="" group="" txn=""  icon="icon-remove"/>
				
	</Operations>
	
	

	<Script></Script>

</CommQry>