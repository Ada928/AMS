<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resources.east.data.pub">
	<class
		name="AmsDszhMessageInfo"
		table="messageinfo"
	>
		<meta attribute="sync-DAO">false</meta>
		
			<id
				name="id"
				column="ID"
				type="int"
			>
			<generator class="increment" />
			</id>
			<property
				name="messagetype"
				column="messagetype"
				type="string"
				length="20"
			/>
			<property
				name="messagename"
				column="messagename"
				type="string"
				length="80"
			/>
			<property
				name="datadate"
				column="datadate"
				type="string"
				length="10"
			/>
			<property
				name="borndate"
				column="borndate"
				type="string"
				length="10"
			/>
			<property
				name="messagestatus"
				column="messagestatus"
				type="string"
				length="2"
			/>
			<property
				name="reserve"
				column="reserve"
				type="string"
				length="20"
			/>
			
	</class>
</hibernate-mapping>